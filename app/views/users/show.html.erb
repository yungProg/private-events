<h1>PRIVATE EVENTS</h1>
<h2>Created Events</h2>
<div><%= link_to "Create an event", new_event_path %></div>
<% @created_events.each do |event| %>
    <ul>
        <li><%= event.theme %></li>
        <li><%= event.event_date %></li>
        <li><%= event.venue %></li>
        <li><%= event.description %></li>
    </ul>
<% end %>

<h2>Attended Events</h2>
<% current_user.attended_events.each do |event| %>
    <p><%= event.theme %></p>
<% end %>